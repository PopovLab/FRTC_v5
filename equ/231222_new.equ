ZEF = 1.9 + 0.8*FRAMP(0.0336,CF3) + 0.08*FRAMP(0.0301,0.0336);
!ZEF = 2.1 + 0.3*FRAMP(CF2,CF3);
!Its densities

NIZ1 = NE*(ZEF-ZMJ)/(ZIM1-ZMJ)/ZIM1;
NIZ2 = NE*(ZEF-ZMJ)/(ZIM2-ZMJ)/ZIM2;

NI = NE*(ZIM1+ZIM2-ZEF)/(ZIM1+ZIM2-ZMJ)/ZMJ;

!NHYDR = NI - NIZ1- NIZ2;
