DSTART = 0.021d0;

CF1 = 1.d-4;
CF2 = 0.0301;
CF3 = 0.0351;
!----------------- Electrons parametrs -----------------
CF14 = 1.5d0;
NE:AS;
CF10 = 1.5d0*(1.d0+0.5d0*FRAMP(CF2,CF3));
CF11 = 0.15d0;
NE = 0.75 * NEX;

TE:AS;
