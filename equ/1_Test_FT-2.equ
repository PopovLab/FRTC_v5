
!----------------- Times -----------------
TSTART = 0.01;
TEND = 0.02;
TINIT = 0.01d0;

DROUT = 1.d-5;
DPOUT = 1.d-5;
DTOUT = 1.d-6;
DSTART = 0.010;

CF1 = 1.d-4;
CF2 = 0.01;
CF3 = 0.011;
!----------------- Electrons parametrs -----------------

NE:AS;
!NE = 2*NEX;
NE = (2.48 - 0.248)*(1-(RHO/ROC)**2) + 0.248;

TE:AS;
TE = 1.2*TEX;
 !TE = (1 - 0.01)*(1-(RHO/ROC)**4) + 0.01;
!----------------- Ion parametrs -----------------

TI:AS; 
TI = (0.5 - 0.01)*(1-(RHO/ROC)**2) + 0.01;

!----------------- Impurities -----------------

AMAIN = AMJ;
ZMAIN = ZMJ;

ZEF = 1.0;

NI = NE;
NDEUT = NI;

!----------------- Plasma parametrs -----------------

!Current densities

CU:EQ;
CU = FPR;

HC = HCSA; DC = DCSA; XC = XCSA;
CC = CNSA; CD = CUBM + 0.*CULH;

!----------------- Equilibrium solver -----------------

NBND = 100;
NEQUIL = 41;

!----------------- LHCD & DrivenCurrent -----------------

QLH = 0.06;

debug:0.00001:DSTART::;
saveprofiles::CF2:CF3:;
!lhcd2017(PELH):CF1:CF2:CF3:;
!drivencurrent(CULH,CAR32,CU)::CF2:CF3:;

CD = CULH;
CCD1 = VINT(PELHB);
CCD2 = IINT(CULHB);

CV10 =  IINT(CU) - IINT(CUBS) - CCD2;
CV11 =  IINT(CUBS);
!----------------- Auxiliary quantities -----------------
CV14 = NEAVB;
CV13 = -TIMDER(CV14);

VTOR = 150000; VPOL = VPSWW;
ER = BTOR*(FRS*MU*VTOR/RTOR+VDIA-VPOL);

 !PE = POH+PEBM-PET*TI; PET = -PEI; 
 !PI = PIBM-PIT*TE; PIT = -PEI;

!----------------- Radial output -----------------
Te\TE; Er\ER; Jpar\CU; shir\SHEAR;
Ne\NE\2; Elong\ELON; Cond\CC; Vpol\ABS(VPOL)/1000;
Ti\TI; E\UPL/RTOR/GP2; Jlh\CULH; q\1./MU;
Ni\NDEUT; Shift\SHIF; Jbs\CUBS; Vdia\VDIA;
!----------------- Time output --------------
q0_1./MU(0); q1_1./MU(ROC);
Ibs_CV11; Iohm_CV10; Ilh_CCD2; Itot_CV10+CCD2+CV11;
Qlh_QLH; Plh_CCD1;











